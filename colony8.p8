pico-8 cartridge // http://www.pico-8.com
version 8
__lua__

function _init()
   t=0
   cur_x=0
   cur_y=0
   surface={}
   objects={}
   phermones={}
   for x=0,16 do
      surface[x]={}
      objects[x]={}
      phermones[x]={}
   end
   surface[8][8]={typ="colony"}
   objects[8][8]={typ="ant"}
   palt(0,false)
   palt(15,true)
end

function _update()
   t+=1
   if btnp(0) and cur_x>0 then
      cur_x-=1
   end
   if btnp(1) and cur_x<15 then
      cur_x+=1
   end
   if btnp(2) and cur_y>0 then
      cur_y-=1
   end
   if btnp(3) and cur_y<15 then
      cur_y+=1
   end
   for x=0,15 do
      for y=0,15 do
         o=objects[x][y]
         if o!=nil and o.t!=t then
            if t%30==0 then
               d=flr(rnd(5))
               dx=x
               dy=y
               if d==0 then
                  dx=x-1
               elseif d==1 then
                  dx=x+1
               elseif d==2 then
                  dy=y-1
               else
                  dy=y+1
               end
               o.t=t
               objects[x][y]=nil
               objects[dx][dy]=o
            end
         end
      end
   end
end

function _draw()
   rectfill(0,0,127,127)
   for x=0,15 do
      for y=0,15 do
         if x==cur_x and y==cur_y then
            spr(3,x*8,y*8)
         end
         s=surface[x][y]
         if s==nil then
            spr(0,x*8,y*8)
         else
            spr(1,x*8,y*8)
         end
         o=objects[x][y]
         if o!=nil then
            spr(2,x*8,y*8)
         end
      end
   end
end
